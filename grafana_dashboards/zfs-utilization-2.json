                      "color": "red",
                      "index": 1,
                      "text": "DEGRADED"
                    },
                    "2": {
                      "color": "red",
                      "index": 2,
                      "text": "FAULTED"
                    },
                    "3": {
                      "color": "red",
                      "index": 3,
                      "text": "OFFLINE"
                    },
                    "4": {
                      "color": "red",
                      "index": 4,
                      "text": "UNAVAIL"
                    },
                    "5": {
                      "color": "red",
                      "index": 5,
                      "text": "REMOVED"
                    },
                    "6": {
                      "color": "red",
                      "index": 6,
                      "text": "SUSPENDED"
                    }
